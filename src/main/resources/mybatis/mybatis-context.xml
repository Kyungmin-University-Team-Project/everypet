<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <!-- Camel Case, Snake Case로만 다를 경우 resultMap을 안 써도 이 코드 쓰면 알아서 바꿔줌 -->
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias type="com.everypet.member.model.vo.Member" alias="Member"></typeAlias>
        <typeAlias type="com.everypet.auth.oauth2.data.domain.OAuth2Member" alias="OAuth2Member"></typeAlias>

        <typeAlias type="com.everypet.member.model.vo.Address" alias="Address"></typeAlias>

        <typeAlias type="com.everypet.point.model.vo.Point" alias="Point"></typeAlias>

        <typeAlias type="com.everypet.product.model.vo.Product" alias="Product"></typeAlias>
        <typeAlias type="com.everypet.product.model.dto.ProductListDTO" alias="ProductListDTO"></typeAlias>
        <typeAlias type="com.everypet.product.model.dto.ProductDTO" alias="ProductDTO"></typeAlias>
        <typeAlias type="com.everypet.product.model.dto.SelectProductDTO" alias="SelectProductDTO"></typeAlias>
        <typeAlias type="com.everypet.product.model.dto.ProductUpdateDTO" alias="ProductUpdateDTO"></typeAlias>

        <typeAlias type="com.everypet.cart.model.vo.Cart" alias="Cart"></typeAlias>
        <typeAlias type="com.everypet.cart.model.dto.CartItemDTO" alias="CartItem"></typeAlias>

        <typeAlias type="com.everypet.product.model.vo.Advertisement" alias="Advertisement"></typeAlias>
        <typeAlias type="com.everypet.product.model.dto.AdvertisementDTO" alias="AdvertisementDTO"></typeAlias>
        <typeAlias type="com.everypet.product.model.dto.InsertAdvertisementDTO" alias="InsertAdvertisementDto"></typeAlias>

        <typeAlias type="com.everypet.product.model.dto.SearchProductDTO" alias="SearchProductDTO"></typeAlias>

        <typeAlias type="com.everypet.order.model.vo.Order" alias="Order"></typeAlias>
        <typeAlias type="com.everypet.order.model.vo.OrderDetail" alias="OrderDetail"></typeAlias>
        <typeAlias type="com.everypet.order.model.dto.OrderDTO$MyOrderListDTO" alias="MyOrderListDTO"/>

        <typeAlias type="com.everypet.keyword.model.dto.KeywordRankDTO" alias="KeywordRankDTO"></typeAlias>
        <typeAlias type="com.everypet.keyword.model.dto.KeywordRankDTO$TopKeywordRank" alias="TopKeywordRank"></typeAlias>

        <typeAlias type="com.everypet.review.model.dto.ReviewDTO$ProductReviewDTO" alias="ProductReviewDTO"/>

        <typeAlias type="com.everypet.support.model.entity.SellerInquiry" alias="SellerInquiry"/>

        <typeAlias type="com.everypet.global.util.PageableList" alias="PageableList"/>

        <package name="com.everypet.keyword.model.vo"></package>

    </typeAliases>

</configuration>